{
	"root": true,
	"extends": [
		"wikimedia",
		"wikimedia/node",
		"wikimedia/language/es2017"
	],
	"env": {
		"mocha": true
	},
	"globals": {
		"$": false,
		"browser": false,
		"mw": false
	},
	"rules": {
		"prefer-template": "off",
		"one-var": [ "error", { "initialized": "never", "uninitialized": "always" } ]
	}
}
